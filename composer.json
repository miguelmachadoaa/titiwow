{
    "name": "alpina-go/alpina-go",
    "description": "Tienda AlpinaGo.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "Privada",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "./infyomlabs/laravel-generator"
        }
    ],
    "require": {
        "php": ">=7.2",
        "arcanedev/log-viewer": "v5.x-dev",
        "cartalyst/sentinel": "^3.0",
        "consoletvs/charts": "6.*",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "6.0.*",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "mercadopago/dx-php": "2.4.2",
        "nicolaslopezj/searchable": "^1.10",
        "spatie/laravel-activitylog": "^3.9.1",
        "spatie/laravel-analytics": "^3.4",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "kint-php/kint": "^2.1",
        "filp/whoops": "~2.0",
        "mockery/mockery": "1.0.*",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan logger:link",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
